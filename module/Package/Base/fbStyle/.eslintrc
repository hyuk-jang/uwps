{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "linebreak-style": "off",
    "quotes": "error",
    "no-console": "off",
    "max-len": "off",
    "no-param-reassign": "off",
    "class-methods-use-this": "off",
    "consistent-return": "off",
    "prettier/prettier": ["error", "fb"]
  }
}